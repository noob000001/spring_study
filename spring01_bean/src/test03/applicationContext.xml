<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="dateBean" class="java.util.Date">
		<property name="year" value="121"/>
		<property name="month" value="9"/>
		<property name="date" value="9"/>
	</bean>	
 
 	<bean id="scoreBean" class="test03.Score">
 		<constructor-arg name="name" value="이규원"/>
 		<constructor-arg name="kor" value="100"/>
 		<constructor-arg name="eng" value="10"/>
 	</bean>
 
	<bean id="beanTest" class="test03.BeanTest">
		<constructor-arg name="date" ref="dateBean"/>
		<property name="score" ref="scoreBean"/>
		<property name="boolean" value="true"/>
		<property name="number" value="10.5"/>
		<property name="array">
			<array>
				<value>홍길동</value>
				<value>이순신</value>
				<value>김선달</value>
			</array>
		</property>
		<property name="list">
			<list>
				<value>봄</value>
				<value>여름</value>
				<value>가을</value>
				<value>겨울</value>
			</list>
		</property>
		<property name="set">
			<set>
				<value>1</value>
				<value>2</value>
				<value>3</value>
				<value>4</value>
			</set>
		</property>
		<property name="map">
			<map>
				<entry>
					<key><value>spring</value></key>
					<value>봄</value>
				</entry>
				<entry key="summer" value="여름"></entry>
				<entry key="fall" value="가을"></entry>
				<entry key="winter" value="겨울"></entry>
			</map>
		</property>
		<property name="properties">
			<props>
			<prop key="sky">하늘</prop>
			<prop key="ground">땅</prop>
			</props>
		</property>
		<property name="scoreList">
			<list>
				<bean class="test03.Score">
					<constructor-arg value="오일남"/>
					<constructor-arg value="100"/>
					<constructor-arg value="100"/>
				</bean>
				<ref bean="scoreBean"/>
			</list>
		
		
		</property>
		
		
		
		
	</bean>
	
	

</beans>
